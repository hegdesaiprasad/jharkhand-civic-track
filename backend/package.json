{
    "name": "jharkhand-civic-track-backend",
    "version": "1.0.0",
    "description": "Backend API for Jharkhand Civic Track application",
    "main": "dist/server.js",
    "type": "module",
    "scripts": {
        "dev": "tsx watch src/server.ts",
        "build": "tsc",
        "start": "node dist/server.js",
        "db:setup": "psql -U postgres -d jharkhand_civic_track -f src/db/schema.sql",
        "db:seed": "tsx src/db/seed.ts"
    },
    "keywords": [
        "civic",
        "track",
        "api"
    ],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "express": "^4.18.2",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "pg": "^8.11.3",
        "bcrypt": "^5.1.1",
        "jsonwebtoken": "^9.0.2"
    },
    "devDependencies": {
        "@types/express": "^4.17.21",
        "@types/cors": "^2.8.17",
        "@types/pg": "^8.10.9",
        "@types/bcrypt": "^5.0.2",
        "@types/jsonwebtoken": "^9.0.5",
        "@types/node": "^20.10.5",
        "typescript": "^5.3.3",
        "tsx": "^4.7.0"
    }
}